/* pages/registe/registe.wxss */
.registe-container{
    
    background: #f8f8f8;
    height: 100vh;
}
.title{
    width: 100%;
    text-align: center;
    font-size: 20px;
    font-weight: bold;
}
.page-section{
    width: 90%;
    margin: 10px auto;
}
.weui-cell{
    width: 100%;
    height: 40px;
    margin-bottom: 15px;
    background: #fff;
    font-size: 13px;
    box-sizing: border-box;
    padding: 5px;
}
.btn-group{
    width: 100%;
    display: flex;
}
.type{
    width: 100%;
    height: 50px;
    display: flex;
    align-items: center;
    justify-content: center;
    box-sizing: border-box;
    padding: 10px 0;
}
.typechange{
    width: 30%;
    height: 100%;
    margin: 0 5px;
    /* text-align: center; */
    /* line-height: 50px; */
    display: flex;
    align-items: center;
    justify-content: center;
    background: #b7b7b7;
    border-radius: 5px;
    color: #000;
    font-weight: bold;
}

.active{
    background: #07c160;
    color: #fff;
}